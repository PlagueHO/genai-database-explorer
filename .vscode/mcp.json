{
    "inputs": [],
    "servers": {
        "agent-framework-docs": {
            "type": "sse",
            "url": "https://gitmcp.io/microsoft/agent-framework"
        },
        "aspire": {
            "type": "stdio",
            "command": "aspire",
            "args": [
                "mcp",
                "start"
            ]
        },
        "context7": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ]
        },
        "github": {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/",
        },
        "microsoft-learn": {
            "type": "http",
            "url": "https://learn.microsoft.com/api/mcp"
        },
        "microsoft-foundry": {
            "type": "http",
            "url": "https://mcp.ai.azure.com"
        },
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@playwright/mcp@latest"
            ],
        }
    }
}